# Environment variables for the frontend application
# Copy this file to .env.development or .env.production and fill in the values.

VITE_API_BASE_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:3001

# Topic for notifying the system of configuration changes (e.g., alert thresholds)
MQTT_CONFIG_UPDATE_TOPIC=air-dome/config/update

# The base MQTT topic for all sensor data
MQTT_SENSORS_TOPIC=air-dome/sensors
